	++ -- & * + - ~ !
	/ % << >> < > <= >= == != ^ | && ||
	? : ; ...
	= *= /= %= += -= <<= >>= &= ^= |=
	, # ##
	<: :> <% %> %: %:%:
*/

int i[10];

void main()
{
	i[0] = 50;
	++i[0];
	--i[0];
	i[1] = &i[0];
	i[1] = *i[1];
	i[1] = -+-i[1];
	i[2] = !i[1];
	i[3] = i[1]/10;
	i[4] = i[1]%10;

	i[5] = ( i[1] < 1 && i[1] <= 1 || i[1] >= 1 );
	i[6] = ( i[1] > 1 || i[1] == 1 && i[1] != 1 );

	i[7] += i[7] -= 1;

	i[8] = i[5] ? i[4] : i[3];

}

